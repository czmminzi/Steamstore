#classify{
	float: left;
	width: 150px;
	margin: 25px;
	padding-top: 25px;
}
#classify>div{
	margin: 15px 0px 15px 0px;	
}
ul{
	list-style: none;
}
#classify li{
	padding-top: 5px;
}

#content{
	float: left;
	width: 1100px;
	margin: 10px 50px;
	}
.menus{
	margin: 30px 0px 10px 0px;
}
.menu1{
  height: 20px;
  margin: 3px 0px;	
  padding: 0px 50px;
}
.menu1>ul>li{
	float: right;	
	height: 20px;
	margin: 0px;
	padding: 0px 35px;
	line-height:20px;
}
.menu1 .lines{
	width: 2px;
	height: 25px;
	margin: 0px;
	padding: 0px;
}
#menu2{
	position: relative;
  height: 35px;	
  margin: 1px 50px;	
  padding: 0px;
}
#menu2>ul>li{
	float: left;
	height: 35px;
	width: 28px;
	margin: 0px;
	padding: 0px 25px;
	line-height: 35px;
	position: relative;
}
#menu2 .lines{
	width: 1px;
	height: 35px;
	margin: 0px;
	padding: 0px;
}
#menu2 #shop{
  width: 55px;	
}
span{
  text-align: center;
}
#menu2 div{
  width: 150px;	
	position: absolute;
	top: 35px;
	left: 0px;
	z-index: 999;
	//visibility:hidden;
	display: none;
}
#menu2 ul{
	margin: 0px;
	padding: 0px;
}
#menu2 div li{
	margin: 5px 5px;
	padding: 0px 15px;
}
#menu2 a{
  display: inline-block;	
  height: 35px;
  width: 140px;
}
#menu2 div .lines{
	height: 2px;
	margin: 0px 10px;
	padding: 0px;
}
#menu2 #search{
  position: absolute;
  top: 0px;
  right: 0px;
  width: 180px;
	padding: 0px;
}
.showPage{
	height: 480px;
	margin: 30px 0px 10px 0px;
}
.forward{
	float: left;
	height: 126px;
	margin:0px;
	padding: 177px 0px 177px 0px;
}
.intro{
	float: left;
	height: 480px;
	margin:0px;
}
.intro>div{
	margin: 0px;
	padding: 5px 0px 5px 0px;
}
.image{
	height: 408px;
	margin: 0px;
	padding: 0px;
}
.image1{
	float: left;
	margin: 0px 8px 8px 0px;
}
.image2{
	float: left;
	margin: 0px 0px 8px 0px;
}
.image3{
	float: left;
	margin: 0px 8px 0px 0px;
}
.image4{
	float: left;
	margin: 0px;
}
.backward{
	float: left;
	height: 126px;
	margin:0px;
	padding: 177px 0px 177px 0px;
}